{
  "name": "@tooly/openapi",
  "version": "0.0.2",
  "description": "OpenAPI specification code generator for creating Tooly packages",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tooly-openapi": "dist/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "swagger",
    "typescript",
    "code-generation",
    "api",
    "cli",
    "tooly",
    "openai",
    "anthropic",
    "ai",
    "tools",
    "vercel-ai-sdk"
  ],
  "author": "Tooly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctate/tooly.git",
    "directory": "packages/openapi"
  },
  "homepage": "https://github.com/ctate/tooly#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@private/typescript-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.3",
    "@types/pluralize": "^0.0.33",
    "@types/yargs": "^17.0.32",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "openapi-types": "^12.1.3",
    "pluralize": "^8.0.0",
    "swagger-parser": "^10.0.3",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "ai": "^4.3.16",
    "openai": "^5.8.2"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  }
}
